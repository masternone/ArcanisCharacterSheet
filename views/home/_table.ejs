<table cellpadding="0" cellspacing="0" border="1" frame="void" rules="all" >
	<thead>
		<tr>
			<th>Name</th>
			<th>Rank</th>
		</tr>
	</thead>
	<tbody><%
		if( character.length > 0 ){
			character.forEach(function( value, key ){
				var stripe = key % 2 == 0 ? 'even' : 'odd'; %>
				<tr class="<%= stripe %>">
					<td><a href="/character/<%= key %>"><%= value.name %></a></td>
					<td><%= value.abbr %></td><%
					if( edit ){ %>
					<td><a href="<%= linkTo_characterEdit %>"><%= linkTo_characterEditText %></a></td>
					<% } %>
				</tr><%
			});
		}
	%> </tbody>
</table>