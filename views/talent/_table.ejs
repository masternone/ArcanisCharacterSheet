<table cellpadding="0" cellspacing="0" border="1" frame="void" rules="all" >
	<thead>
		<tr>
			<th>Name</th>
			<th>Tallent Group</th>
		</tr>
	</thead>
	<tbody><%
		if( talent.length > 0 ){
			talent.forEach(function( value, key ){
				var stripe = key % 2 == 0 ? 'even' : 'odd'; %>
				<tr class="<%= stripe %>">
					<td><a href="/talent/<%= key %>"><%= value.name %></a></td>
					<% if( edit ){ %>
					<td><a href="<%= linkTo_talentEdit %>"><%= linkTo_talentEditText %></a></td>
					<% } %>
				</tr><%
			});
		}
	%></tbody>
</table>
