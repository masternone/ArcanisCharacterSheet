<table cellpadding="0" cellspacing="0" border="1" frame="void" rules="all" >
	<thead>
		<tr>
			<th>Name</th>
			<th>Abbrevation</th>
		</tr>
	</thead>
	<tbody><%
		if( archetype.length > 0 ){
			archetype.forEach(function( value, key ){
				var stripe = key % 2 == 0 ? 'even' : 'odd'; %>
				<tr class="<%= stripe %>">
					<td><a href="/archetype/<%= key %>"><%= value.name %></a></td>
					<td><%= value.abbr %></td><%
					if( edit ){ %>
					<td><a href="<%= linkTo_archetypeEdit %>">linkTo_archetypeEditText</a></td>
					<% } %>
					%>
				</tr><%
			});
		}
	%> </tbody>
</table>