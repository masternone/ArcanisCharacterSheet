<table cellpadding="0" cellspacing="0" border="1" frame="void" rules="all" >
	<thead>
		<tr>
			<th>Name</th>
			<th>Attribute</th>
			<th>Group</th>
		</tr>
	</thead>
	<tbody><%
	console.log( skill );
		if( skill.length > 0 ){
			skill.forEach(function( value, key ){
				var stripe = key % 2 == 0 ? 'even' : 'odd'; %>
				<tr class="<%= stripe %>">
					<td><a href="/skill/<%= key %>"><%= value.name %></a></td>
					<td><%= attribute[value.attribute].name %></td>
					<td><%= skillGroup[value.skillGroup].name %></td>
					<% if( edit ){ %>
					<td><a href="<%= linkTo_skillEdit %>"><%= linkTo_skillEditText %></a></td>
					<% } %>
				</tr><%
			});
		}
	%> </tbody>
</table>