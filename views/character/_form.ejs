<% console.log( character ) %>
<form id="<%= form.name %>" name="<%= form.name %>" action="<%= form.action %>" method="POST" >
	<% if( typeof( character.id ) == 'number' ){ %><input type="hidden" id="id" name="id" value="<%= character.id %>" /> <% } %>
	<% if( form.method == "put" ){ %><input type="hidden" id="_method" name="_method" value="<%= method %>" /> <% } %>
	<p>
		<label for="name" >Name</label>
		<input type="text" id="name" name="name" value="<%= character.name %>" />
	</p>
	<div class="clearfix" ></div>
	<p>
		<span class="label">Attribute</span>
		<% console.log( 'character.attribute', character.attribute );
		for( n in character.attribute ){
			console.log( 'n', n );
			console.log( 'character.attribute[n]', character.attribute[n] ); %>
			<label for="<%= character.attribute[n].abbr %>" ><%= character.attribute[n].name %></label>
			<input type="number" id="<%= character.attribute[n].abbr %>" name="<%= character.attribute[n].abbr %>" value="2" />
		<% } %>
	</p>
	<div class="clearfix" ></div>
	<p>
		<input type="submit" id="submit" name="submit" value="<%= form.submit %>" />
		<input type="reset" id="reset" name="reset" value="Reset" />
	</p>
	<div class="clearfix" ></div>
</form>